{
  "name": "express-web-components",
  "version": "0.4.2",
  "description": "Declaratively build Express applications with web components.",
  "keywords": [
    "web-components"
  ],
  "scripts": {
    "dev": "node dev-init.js && electron node_modules/scram-engine/main.js example/app/server/index.html",
