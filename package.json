    "test": "electron node_modules/scram-engine/main.js test/index.html -d -p 5052"
  },
  "license": "MIT",
  "devDependencies": {
    "axios": "^0.12.0",
    "chai": "^3.5.0",
    "cors": "^2.7.1",
    "electron-prebuilt": "^1.2.6",
    "es-no-build": "^0.1.0",
    "express": "^4.14.0",
    "mocha": "^2.5.3",
    "scram-engine": "^0.3.1"
  },
  "dependencies": {}
}
